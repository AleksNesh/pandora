<config>
    <tabs>
        <icon_divider module="ash_devbar">
            <block>ash_devbar/tab_divider</block>
            <options>
                <css>
                    <class1>divider</class1>
                </css>
            </options>
        </icon_divider>
        <version translate ="label" module="ash_devbar">
            <label>Magento Version</label>
            <block>ash_devbar/tab_version</block>
            <options>
                <css>
                    <class1>has-info</class1>
                </css>
            </options>
        </version>
        <time translate="label" module="ash_devbar">
            <label>Profiler Runtime</label>
            <block>ash_devbar/tab_time</block>
            <options>
                <icon>fa-clock-o</icon>
                <css>
                    <class1>has-info</class1>
                </css>
            </options>
        </time>
        <memory translate="label" module="ash_devbar">
            <label>Memory Used</label>
            <block>ash_devbar/tab_memory</block>
            <options>
                <icon>fa-bar-chart-o</icon>
                <css>
                    <class1>has-info</class1>
                </css>
            </options>
        </memory>
        <button_divider module="ash_devbar">
            <block>ash_devbar/tab_divider</block>
            <options>
                <css>
                    <class1>divider</class1>
                </css>
            </options>
        </button_divider>
        <system translate="label" module="ash_devbar">
            <label>System</label>
            <block>ash_devbar/tab_system</block>
            <options>
                <icon>fa-gears</icon>
                <css>
                    <class1>has-form</class1>
                </css>
            </options>
        </system>
    </tabs>
</config>
