<config>
    <modules>
        <Pan_Core>
             <version>0.1.0</version>
        </Pan_Core>
    </modules>

    <global>
        <helpers>
            <pan_core>
                <class>Pan_Core_Helper</class>
            </pan_core>
        </helpers>
        <blocks>
            <pan_core>
                <class>Pan_Core_Block</class>
            </pan_core>
        </blocks>
    </global>

    <adminhtml>
        <translate>
            <modules>
                <Pan_Core>
                    <files>
                        <default>Pan_Core.csv</default>
                    </files>
                </Pan_Core>
            </modules>
        </translate>
    </adminhtml>

</config>
