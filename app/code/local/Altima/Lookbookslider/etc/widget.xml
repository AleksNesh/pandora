<widgets>
    <lookbookslider_widget type="lookbookslider/content_widget">
        <name>Lookbookslider</name>
        <description type="desc">Adds a Lookbook slider</description>
        <parameters>
            <slider_id type="complex" translate="label">
                <required>1</required>
                <visible>1</visible>
                <label>Select Slider</label>
                <type>select</type>
                <source_model>lookbookslider/config_source_sliders</source_model>
            </slider_id>
        </parameters>
    </lookbookslider_widget>
</widgets>