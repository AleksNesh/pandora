            <ash_slideshowextended>
                <class>Ash_SlideshowExtended_Block</class>
            </ash_slideshowextended>
        </blocks>

        <helpers>
            <ash_slideshowextended>
                <class>Ash_SlideshowExtended_Helper</class>
            </ash_slideshowextended>
        </helpers>

        <models>
            <ash_slideshowextended>
                <class>Ash_SlideshowExtended_Model</class>
            </ash_slideshowextended>
        </models>
    </global>
